{
  "root": true,
  "extends": [
      "eslint:recommended"
      // "plugin:jsdoc/recommended"
  ],
  "plugins": [
    "jsdoc"
  ],
  "env": {
    "browser": true,
    "es2021": true,
    "es6": true
  },
  "parserOptions": { 
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-var": "error",
    "prefer-const": "error",
    "no-unused-vars": ["error", { "varsIgnorePattern": "^_", "argsIgnorePattern": "^_", "destructuredArrayIgnorePattern": "^_" }],
    "no-prototype-builtins": "off",
    "jsdoc/no-undefined-types": 1
  }
}